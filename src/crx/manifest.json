{
  "name": "Yammer URL Shortener",
  "version": "1.0",
  "description": "Allows the easy creation of shortlinks.",
  "permissions": [
    "storage",
    "tabs"
  ],
  "browser_action": {
    "default_icon": {
      "16": "./assets/64x64.png",
      "32": "./assets/64x64.png",
      "128": "./assets/64x64.png"
    }
  },
  "background": {
    "scripts": [
      "background/popup/handlePopup.background.js",
      "background/messageHandlers/login.js",
      "background/messageHandlers/shortlink.js",
      "background/messageHandlers/tabs.js"
    ],
    "persistent": false
  },
  "author": "iamtheyammer",
  "manifest_version": 2
}