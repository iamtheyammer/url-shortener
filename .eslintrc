{
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "extends": "eslint:recommended",
  "plugins": [
    "react"
  ],
  "rules": {
    "no-console": 0,
    "quotes": [
      2,
      "single"
    ],
    "semi": [
      2,
      "always"
    ]
  },
  "parserOptions": {
    "sourceType": "module"
  }
}